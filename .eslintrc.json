{
	"root": true,
	"extends": [
		"wikimedia/common",
		"wikimedia/language/es5"
	],
	"env": {
		"shared-node-browser": true
	},
	"globals": {
		"unicodeJS": false
	},
	"rules": {
		"max-len": 0
	},
	"plugins": [ "html" ],
	"settings": {
		"html/indent": "+tab",
		"html/report-bad-indent": "error"
	}
}
